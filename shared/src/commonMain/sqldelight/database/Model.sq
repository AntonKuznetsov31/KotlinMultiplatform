CREATE TABLE userEntity(
	id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	firstName TEXT NOT NULL,
	lastName TEXT NOT NULL
);

getAllUsers:
SELECT *
FROM userEntity;

getUserById:
SELECT *
FROM userEntity
WHERE id = ?;

insertUser:
INSERT OR REPLACE
INTO userEntity(
	id,
	firstName,
	lastName
) VALUES(?, ?, ?);

deleteUserById:
DELETE FROM userEntity
WHERE id = ?;